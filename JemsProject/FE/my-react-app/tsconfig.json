{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    // ここを追加
    "baseUrl": "src",
    "paths": {
      "@utils/*": ["utils/*"],
    }
  },
  "include": ["src"]
}
